<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:transfers="clr-namespace:Zafiro.Avalonia.FileExplorer.NextGen.Core.ViewModels.Transfers"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Zafiro.Avalonia.FileExplorer.NextGen.Core.ViewModels.Transfers.TransferManagerView"
             x:CompileBindings="True"
             x:DataType="transfers:TransferManager">
    <Border Padding="10" Background="Red">
        <ListBox ItemsSource="{Binding Transfers}" DisplayMemberBinding="{Binding Key}" />
    </Border>
</UserControl>