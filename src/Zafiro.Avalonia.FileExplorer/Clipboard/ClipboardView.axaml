<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:clipboard="clr-namespace:Zafiro.Avalonia.FileExplorer.Clipboard"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Zafiro.Avalonia.FileExplorer.Clipboard.ClipboardView"
             x:DataType="clipboard:IClipboard">
	<ItemsControl ItemsSource="{Binding Contents}" />
</UserControl>
